      <th>User</th><th>Date</th><th>Amount</th><th>Status</th><th>Order</th>
    </tr>
  </thead>
  <tbody>
    <% payments.forEach(p => { %>
      <tr>
        <td><%= p.details.userName %></td>
        <td><%= p.createdAt.toLocaleDateString() %></td>
        <td>â‚¹<%= p.amount %></td>
        <td><%= p.status %></td>
        <td><%= p.details.orderName %></td>
      </tr>
    <% }) %>
  </tbody>
</table>

    
</body>
</html>